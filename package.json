    "koa-router": "^7.4.0",
    "koa2-cors": "^2.0.6",
    "lodash": "^4.17.21",
    "net": "^1.0.2",
    "path": "^0.12.7",
    "socket.io": "^2.2.0",
    "socket.io-msgpack-parser": "^2.2.0",
    "source-map": "^0.7.3",
    "source-map-support": "^0.5.10",
